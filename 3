#include <iostream>
using namespace std;

int main() {
    int size;
    cin >> size;

    int arr[size];
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }

    int minimum = arr[0];
    for (int i = 1; i < size; i++) {
        if (arr[i] < minimum) {
            minimum= arr[i];
        }
    }

    cout << "MIn=" << minimum<< endl;

    return 0;
}
