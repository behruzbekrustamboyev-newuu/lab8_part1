#include <iostream>
#include <iomanip>
using namespace std;
int main() {
    int n;
    cout<<"size";
    cin >> n;
    double arr[n], sum = 0;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
        sum += arr[i];

    }
    cout<<"average number of elements: ";
    cout << sum / n;
    return 0;
}
